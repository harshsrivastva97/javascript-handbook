{
    "compilerOptions": {
      "module": "ESNext",
      "moduleResolution": "Node",
      "resolveJsonModule": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "strict": true,
      "baseUrl": "./",
      "paths": {
        "*": ["src/*"]
      },
      "noEmit": true,
      "allowImportingTsExtensions": true,
      "jsx": "react-jsx"
    }
  }